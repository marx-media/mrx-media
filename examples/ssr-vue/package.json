{
  "name": "@marx-media/example-ssr-vue",
  "version": "1.0.0",
  "author": "Dominic Marx <hey@marx-media.net>",
  "type": "module",
  "scripts": {
    "build": "npx simple-ssr build",
    "dev:express": "NODE_ENV=development node --no-warnings --experimental-loader=@mrx-media/vite-vue-simple-ssr/loader --experimental-json-modules src/serverExpress.ts",
    "dev:fastify": "NODE_ENV=development node --no-warnings --experimental-loader=@mrx-media/vite-vue-simple-ssr/loader --experimental-json-modules src/serverFastify.ts",
    "start:express": "NODE_ENV=production node --no-warnings --experimental-loader=@mrx-media/vite-vue-simple-ssr/loader --experimental-json-modules src/serverExpress.ts",
    "start:fastify": "NODE_ENV=production node --no-warnings --experimental-loader=@mrx-media/vite-vue-simple-ssr/loader --experimental-json-modules src/serverFastify.ts"
  },
  "dependencies": {
    "@mrx-media/vite-vue-simple-ssr": "workspace:^1.0.0",
    "@vueuse/head": "^0.7.6",
    "express": "^4.18.1",
    "fastify": "^3.29.0",
    "pino-pretty": "^7.6.1",
    "vue": "^3.2.34",
    "vue-router": "^4.0.15"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@vitejs/plugin-vue": "^2.3.3",
    "commander": "^9.2.0",
    "typescript": "4.7.0-beta",
    "vite": "^2.9.9"
  }
}

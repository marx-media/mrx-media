{
  "name": "@mrx-media/directus-extension-vue-ssr",
  "version": "1.0.0",
  "author": "Dominic Marx <hey@marx-media.net>",
  "type": "module",
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "build:deploy": "tsup --onSuccess \"node scripts/postbuild.js\""
  },
  "keywords": [
    "directus",
    "directus-extension",
    "directus-hook",
    "vue",
    "ssr"
  ],
  "directus:extension": {
    "type": "hook",
    "path": "dist/index.js",
    "source": "src/index.ts",
    "host": "^v9.10.0"
  },
  "dependencies": {
    "@directus/extensions-sdk": "^9.10.0",
    "@mrx-media/vite-vue-simple-ssr": "workspace:^1.0.0",
    "axios": "^0.27.2",
    "express": "^4.18.1",
    "geojson": "^0.5.0",
    "knex": "^2.0.0",
    "knex-schema-inspector": "^1.7.5",
    "pino": "^7.11.0",
    "vue": "^3.2.34",
    "vue-i18n": "^9.1.10",
    "vue-router": "^4.0.15"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/geojson": "^7946.0.8",
    "@types/pino": "^7.0.5",
    "directus-codestyle": "github:dimitrov-adrian/directus-codestyle",
    "eslint": "^8.12.0",
    "postcss-html": "^1.4.1",
    "prettier": "^2.6.2",
    "stylelint": "^14.8.2",
    "tsup": "^5.12.8",
    "typescript": "4.7.0-beta"
  },
  "prettier": "directus-codestyle/prettier",
  "stylelint": {
    "extends": "directus-codestyle/stylelint"
  },
  "eslintConfig": {
    "extends": "./node_modules/directus-codestyle/.eslintrc.js",
    "parserOptions": {
      "sourceType": "module"
    }
  }
}
